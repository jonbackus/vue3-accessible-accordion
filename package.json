{
	"name": "vue3-accessible-accordions",
	"version": "0.5.0",
	"description": "A Vue 3 plugin offering a WAI-ARIA compliant accordion design pattern implementation",
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build --mode production --target lib --name index ./src/index.js",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"core-js": "^3.6.5",
		"vue": "^3.0.0"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.5.0",
		"@vue/cli-plugin-eslint": "~4.5.0",
		"@vue/cli-service": "~4.5.0",
		"@vue/compiler-sfc": "^3.0.0",
		"@vue/eslint-config-prettier": "^6.0.0",
		"babel-eslint": "^10.1.0",
		"eslint": "^6.7.2",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-vue": "^7.0.0-0",
		"prettier": "^1.19.1"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/vue3-essential",
			"eslint:recommended",
			"@vue/prettier"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {}
	},
	"rules": {
		"prettier/prettier": [
			"warn",
			{
				"singleQuote": true,
				"semi": true,
				"trailingComma": "es5"
			}
		]
	},
	"prettier": {
		"useTabs": true,
		"tabWidth": 4,
		"singleQuote": true,
		"trailingComma": "all",
		"printWidth": 120
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	],
	"homepage": "https://github.com/jonbackus/vue3-accessible-accordion",
	"repository": {
		"type": "git",
		"url": "https://github.com/jonbackus/vue3-accessible-accordion.git"
	},
	"license": "MIT"
}
